{
    "name": "loco",
    "version": "0.0.0",
    "description": "An Electron application with Vue and TypeScript",
    "main": "./out/main/index.js",
    "author": "lopo",
    "scripts": {
        "start": "electron-vite preview",
        "dev:loco": "electron-vite dev",
        "build:web": "tsc --noEmit && electron-vite build",
        "build:win": "yarn run build build && electron-builder --win --config",
        "build:mac": "yarn run build && electron-builder --mac --config",
        "build:linux": "yarn run build && electron-builder --linux --config"
    },
    "dependencies": {
        "@electron-toolkit/preload": "^1.0.1",
        "@electron-toolkit/utils": "^1.0.2"
    },
    "devDependencies": {
        "@electron-toolkit/tsconfig": "^1.0.1",
        "@types/node": "16.11.26",
        "@vitejs/plugin-vue": "^2.3.1",
        "electron": "^18.0.3",
        "electron-builder": "^23.0.3",
        "electron-notarize": "^1.2.1",
        "electron-vite": "^1.0.2",
        "sass": "^1.50.0",
        "typescript": "^4.5.5",
        "vite": "^2.9.1",
        "vue": "^3.2.31"
    }
}
