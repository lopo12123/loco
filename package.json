{
    "name": "loco",
    "private": true,
    "version": "0.0.1-dev",
    "description": "Simple desktop git app",
    "main": "./out/main/index.js",
    "author": "lopo",
    "license": "MIT",
    "scripts": {
        "start": "electron-vite preview",
        "dev:loco": "electron-vite dev",
        "build": "tsc --noEmit && electron-vite build",
        "build:win": "yarn run build && electron-builder --win --config",
        "build:mac": "yarn run build && electron-builder --mac --config",
        "build:linux": "yarn run build && electron-builder --linux --config"
    },
    "dependencies": {
        "@electron-toolkit/preload": "^1.0.1",
        "@electron-toolkit/utils": "^1.0.2",
        "primeicons": "^5.0.0",
        "primevue": "^3.12.4",
        "simple-git": "^3.6.0",
        "vue-router": "^4.0.14"
    },
    "devDependencies": {
        "@electron-toolkit/tsconfig": "^1.0.1",
        "@types/node": "16.11.26",
        "@vitejs/plugin-vue": "^2.3.1",
        "electron": "^18.0.3",
        "electron-builder": "^23.0.3",
        "electron-notarize": "^1.2.1",
        "electron-vite": "^1.0.2",
        "sass": "^1.50.0",
        "typescript": "^4.5.5",
        "vite": "^2.9.1",
        "vue": "^3.2.31"
    }
}
